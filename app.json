{
  "expo": {
    "name": "Cita-connect",
    "slug": "Cita-connect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/logo_cita_connect_orange_transparent.png",
    "splash": {
      "image": "",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/logo_cita_connect_orange_transparent.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/img/logo_cita_connect_orange_transparent.png"
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": ["js", "jsx", "scss", "sass"]
    }
  }
}
